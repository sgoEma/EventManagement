<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Event_Not_Registered</fullName>
    <active>true</active>
    <errorConditionFormula>AND(IsEventRegistered__c = FALSE,  RecordType.Name  &lt;&gt; &quot;Speaker Registration&quot;)</errorConditionFormula>
    <errorMessage>Attendee must have active registration to related event</errorMessage>
</ValidationRule>
